<!DOCTYPE html>
<html lang="en">
<head>
<title>PESTICIDES SUPPLY SYSTEM | Contacts</title>
<meta charset="utf-8">
<link rel="stylesheet" href="css/style.css">

<script src="js/superfish.js"></script>
<script src="js/jquery.easing.1.3.js"></script>
<script src="js/tms-0.4.1.js"></script>
<script src="js/slider.js"></script>
<!--[if lt IE 9]>
<script src="js/html5.js"></script>
<link rel="stylesheet" href="css/ie.css"> 
<![endif]-->

</head>
<body>
<div class="main-bg">
  <!-- Header -->
  <header>
    <div class="inner">
      <h1 class="images/pesticide logo.jpg"><a href="/home" style="color: black;">PESTICIDES SUPPLY SYSTEM</a></h1>
      
      <div class="clear"></div>
    </div>
  </header>
  <!-- Content -->
  <section id="content">
    <div class="container_24">
      <div class="wrapper">
        <div class="grid_24 content-bg">
          <div class="wrapper">
            <article class="grid_6 suffix_1 prefix_1 alpha">
              <h2>USERS REVIEW:</h2>
              
                
<body>
  
    <button style="margin-left: 700px; width: 150px; " ><li> <a style="color: black; width: 100px; text-decoration: none;" href="/"> LOG OUT</a></li></button>
     
    <div style="margin-left: 700px; width: 150px;"><a style="text-decoration: none;width: 100px; background-color: white; color: black;" href="/adminview">PRODUCTS</a></div>
    <div style="margin-left: 700px; width: 150px;"><a style="text-decoration: none;width: 100px; background-color: white; color: black;" href="/addproduct">ADD PRODUCTS</a></div>
    
    <div>
                        
        <% if (reviews && reviews.length > 0) { %>
         <div id="reviewListContainer">
         <ul>
        <% reviews.forEach(function(review) { %>
        <div style="width: 500px; height: 50px; border: solid white; margin: 20px; background-color: yellow;">
            <li> <%= review.name %></li>
            <li> <%= review.message %></li>
        </div>
        <% }); %>
        </ul>
        </div>
<% } else { %>
<p>No reviews available</p>
<% } %>
</div>

</body>
</html>
